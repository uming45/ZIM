<?xml version="1.0" encoding="utf-8"?>
<animation-list 
    xmlns:android="http://schemas.android.com/apk/res/android"   
	android:oneshot="false">
    <!-- 设置Android:oneshot属性为true,表示此次动画只执行一次，最后停留在最后一帧。设置为false则动画循环播放。 -->
    <item  android:drawable="@drawable/gxz" android:duration="200"/>
    <item  android:drawable="@drawable/gxy" android:duration="200"/>
    <item  android:drawable="@drawable/gxx" android:duration="200"/>
</animation-list>
